{
    "name": "swri-sodas/sodas-helper",
    "description": "SodasHelper plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0",
        "dereuromark/cakephp-templating": "@stable",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "SodasHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
